{
  "extends": ["tslint-config-airbnb", "tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules",
      "dist",
      "webpack.config.js"
    ]
  },
  "rules": {
    "jsx-no-multiline-js": false,
    "quotemark": false,
    "variable-name": false,
    "no-implicit-dependencies": false,
    "jsx-no-lambda": false
  }
}